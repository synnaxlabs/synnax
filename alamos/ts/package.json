{
  "name": "@synnaxlabs/alamos",
  "version": "0.39.0",
  "type": "module",
  "description": "Distributed instrumentation for Synnax",
  "repository": "https://github.com/synnaxlabs/synnax/tree/main/freighter/ts",
  "license": "MIT",
  "keywords": [
    "synnax",
    "grpc",
    "transport",
    "data acquisition",
    "big data",
    "telemetry",
    "control systems"
  ],
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "watch": "tsc --noEmit && vite build --watch",
    "test": "vitest",
    "lint": "eslint --cache",
    "fix": "eslint --cache --fix"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@synnaxlabs/x": "workspace:*"
  },
  "devDependencies": {
    "@synnaxlabs/tsconfig": "workspace:*",
    "@synnaxlabs/vite-plugin": "workspace:*",
    "eslint": "^9.18.0",
    "eslint-config-synnaxlabs": "workspace:*",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vitest": "^2.1.8"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dev": {
      "types": "./dist/dev/index.d.ts",
      "import": "./dist/dev.js",
      "require": "./dist/dev.cjs"
    }
  }
}
