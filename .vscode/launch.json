{
  "configurations": [
    {
      "name": "Console: Debug in Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}",
      "preLaunchTask": "console: start dev server",
      "trace": true,
      "sourceMaps": true,
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ],
      "sourceMapPathOverrides": {
        "http://localhost:5173/src/*": "${workspaceFolder}/console/src/*",
        "http://localhost:5173/@fs/*": "/*",
      },
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/**"
      ],
    }
  ]
}