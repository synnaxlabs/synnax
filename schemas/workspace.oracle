struct Workspace {
    field key uuid {
        domain id
    }

    field name string {
        domain validate {
            required
        }
    }

    field layout json

    domain ts {
        output "client/ts/src/workspace"
    }

    domain ontology {
        type "workspace"
    }
}

struct New {
    field key uuid?

    field name string {
        domain validate {
            required
        }
    }

    field layout json

    domain ts {
        output "client/ts/src/workspace"
        use_input
    }
}
