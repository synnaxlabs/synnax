@ts output "client/ts/src/workspace/log"
@go output "core/pkg/service/workspace/log"
@pb

Key = uuid

Log struct {
    key  Key    @key
    name string
    data json

    @ontology type "log"
    @go omit
    @pb omit
}

GoLog struct extends Log {
    data string
    -data

    @go name "Log"
    @pb name "Log"
    @ts omit
}

New struct extends Log {
    key  Key?
    data json @ts stringify

    @ts use_input
    @go omit
    @pb omit
}
