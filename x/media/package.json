{
  "name": "@synnaxlabs/media",
  "version": "0.43.0",
  "type": "module",
  "description": "Media Assets for Synnax Labs",
  "repository": "https://github.com/synnaxlabs/synnax/tree/main/x/media",
  "keywords": [
    "synnax",
    "media",
    "data acquisition",
    "big data",
    "telemetry",
    "control systems"
  ],
  "scripts": {
    "build": "vite build",
    "watch": "vite build --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint --cache",
    "fix": "eslint --cache --fix",
    "madge": "madge --circular src"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "react": "^19.1.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@synnaxlabs/tsconfig": "workspace:*",
    "@synnaxlabs/vite-plugin": "workspace:*",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.8",
    "@vitejs/plugin-react": "^4.5.2",
    "eslint": "^9.28.0",
    "eslint-config-synnaxlabs": "workspace:*",
    "madge": "^8.0.0",
    "vite": "^6.3.5"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/media.cjs",
  "module": "./dist/media.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/media.js",
      "require": "./dist/media.cjs"
    },
    "./dist/*": "./dist/*"
  }
}
