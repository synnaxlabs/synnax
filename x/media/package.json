{
  "name": "@synnaxlabs/media",
  "version": "0.40.0",
  "type": "module",
  "description": "Media Assets for Synnax Labs",
  "repository": "https://github.com/synnaxlabs/synnax/tree/main/x/media",
  "keywords": [
    "synnax",
    "media",
    "data acquisition",
    "big data",
    "telemetry",
    "control systems"
  ],
  "scripts": {
    "build": "vite build",
    "watch": "vite build --watch",
    "lint": "eslint --cache",
    "fix": "eslint --cache --fix",
    "madge": "madge --circular src"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "react": "^19.0.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@synnaxlabs/tsconfig": "workspace:*",
    "@synnaxlabs/vite-plugin": "workspace:*",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "eslint-config-synnaxlabs": "workspace:*",
    "madge": "^8.0.0",
    "vite": "^6.2.1"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/media.cjs",
  "module": "./dist/media.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/media.js",
      "require": "./dist/media.cjs"
    },
    "./dist/*": "./dist/*"
  }
}
