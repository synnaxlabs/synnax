{
  "name": "@synnaxlabs/media",
  "version": "0.45.0",
  "type": "module",
  "description": "Media Assets for Synnax Labs",
  "repository": "https://github.com/synnaxlabs/synnax/tree/main/x/media",
  "keywords": [
    "synnax",
    "media",
    "data acquisition",
    "big data",
    "telemetry",
    "control systems"
  ],
  "scripts": {
    "build": "vite build",
    "watch": "vite build --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint",
    "fix": "eslint --fix",
    "madge": "madge --circular src"
  },
  "dependencies": {
    "clsx": "catalog:",
    "react": "catalog:",
    "react-icons": "catalog:"
  },
  "devDependencies": {
    "@synnaxlabs/tsconfig": "workspace:^",
    "@synnaxlabs/vite-plugin": "workspace:^",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "eslint": "catalog:",
    "eslint-config-synnaxlabs": "workspace:^",
    "madge": "catalog:",
    "vite": "catalog:"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/media.cjs",
  "module": "./dist/media.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/media.js",
      "require": "./dist/media.cjs"
    },
    "./dist/*": "./dist/*"
  }
}
