---
import Inline from "@/components/code/Inline.astro";
import { fetchVersion } from "@/util/fetchVersion";
const { os } = Astro.props;

const version = await fetchVersion();
---

<Inline
    lang="bash"
    code={`sudo mv synnax-v${version.trim()}-${os.toLowerCase()} /usr/local/bin/synnax`}
    theme="css-variables"
/>
