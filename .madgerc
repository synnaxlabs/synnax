{
    "fileExtensions": [
        "ts",
        "tsx",
        "js",
        "jsx"
    ],
    "tsConfig": "tsconfig.json",
    "detectiveOptions": {
        "ts": {
            "skipTypeImports": true
        },
        "tsx": {
            "skipTypeImports": true
        }
    },
    "excludeRegExp": [
        ".*\\.d\\.ts$"
    ]
}
