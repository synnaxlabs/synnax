{
  "cluster": {
    "insecure": true,
    "mem_fs": true
  },
  "setup": {
    "index_channels": 3,
    "data_channels": 6,
    "client": "py"
  },
  "steps": [
    [
      {
        "op": "write",
        "client": "py",
        "params": {
          "channel_groups": [
            {
              "index_channels": [
                "int0",
                "int1",
                "int2"
              ],
              "data_channels": [
                "int0-0",
                "int0-1",
                "int1-0",
                "int1-1",
                "int2-0",
                "int2-1"
              ]
            }
          ],
          "num_writers": 1,
          "domains": 100000,
          "samples_per_domain": 1000,
          "time_range": {
            "start": 0,
            "end": 1000000000000
          },
          "writer_mode": 2,
          "expected_error": "no_error"
        }
      }
    ],
    [
      {
        "op": "delete",
        "client": "ts",
        "params": {
          "channels": [
            "int0",
            "int1",
            "int2",
            "int0-0",
            "int0-1",
            "int1-0",
            "int1-1",
            "int2-0",
            "int2-1"
          ],
          "time_range": {
            "start": 1000000000,
            "end": 300000000000
          },
          "expected_error": "no_error"
        }
      },
      {
        "op": "delete",
        "client": "py",
        "params": {
          "channels": [
            "int0",
            "int1",
            "int0-0",
            "int0-1",
            "int1-0",
            "int1-1"
          ],
          "time_range": {
            "start": 300000000000,
            "end": 1001000000000
          },
          "expected_error": "no_error"
        }
      },
      {
        "op": "delete",
        "client": "ts",
        "params": {
          "channels": [
            "int2",
            "int2-1",
            "int2-0",
            "int0-1",
            "int0-0"
          ],
          "time_range": {
            "start": 200000000000,
            "end": 1001000000000
          },
          "expected_error": "no_error"
        }
      }
    ],
    [
      {
        "op": "read",
        "client": "ts",
        "params": {
          "channel_groups": [
            [
              "int0",
              "int1",
              "int2",
              "int0-0",
              "int0-1",
              "int1-0",
              "int1-1",
              "int2-0",
              "int2-1"
            ]
          ],
          "num_iterators": 1,
          "chunk_size": 100000,
          "bounds": {
            "start": 0,
            "end": 1001000000000
          },
          "expected_error": "no_error",
          "samples_expected": 900000
        }
      }
    ]
  ],
  "cleanup": {
    "delete_all_channels": true,
    "client": "py"
  }
}