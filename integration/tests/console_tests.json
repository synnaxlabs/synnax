{
  "$schema": "test_schema.json",
  "sequences": [
    {
      "sequence_name": "workspace",
      "tests": [
        { "case": "console/workspace/workspace" },
        { "case": "console/workspace/ontology" }
      ]
    },
    {
      "sequence_name": "lifecycle",
      "sequence_order": "asynchronous",
      "pool_size": 3,
      "tests": [
        { "case": "console/label/label_lifecycle" },
        { "case": "console/rack/rack_lifecycle" },
        { "case": "console/range/range_lifecycle" },
        { "case": "console/schematic/schematic_lifecycle" },
        { "case": "console/schematic/custom_symbols" }
      ]
    },
    {
      "sequence_name": "pages",
      "sequence_order": "asynchronous",
      "pool_size": 3,
      "tests": [
        { "case": "console/pages/open_close" },
        { "case": "console/pages/rename_synchronization" },
        { "case": "console/log/log_lifecycle" },
        { "case": "console/plot/line_plot" },
        { "case": "console/schematic/alignment" },
        { "case": "console/schematic/edit_props" },
        { "case": "console/table/table_lifecycle" },
        { "case": "console/range/range_details" },
        { "case": "console/range/range_explorer" }
      ]
    },
    {
      "sequence_name": "channel",
      "tests": [
        { "case": "console/channel/ontology" },
        { "case": "console/channel/channel_lifecycle" },
        { "case": "console/channel/rename_synchronization" },
        { "case": "console/channel/alias_synchronization" },
        {
          "case": "console/channel/calc_channel_stress",
          "parameters": { "rate": [10, 100, 1000] }
        }
      ]
    },
    {
      "sequence_name": "user",
      "sequence_order": "asynchronous",
      "pool_size": 3,
      "tests": [
        { "case": "console/user/register" },
        { "case": "console/user/assign_role" },
        { "case": "console/user/drag_to_role" },
        { "case": "console/user/login_logout" },
        { "case": "console/user/logout_badge" }
      ]
    },
    {
      "sequence_name": "roles",
      "sequence_order": "asynchronous",
      "pool_size": 3,
      "tests": [
        { "case": "console/user/role_engineer_permissions" },
        { "case": "console/user/role_operator_permissions" },
        { "case": "console/user/role_viewer_permissions" }
      ]
    },
    {
      "sequence_name": "tasks",
      "sequence_order": "asynchronous",
      "pool_size": 3,
      "tests": [
        { "case": "console/task/ni_analog_write_forms" },
        { "case": "console/task/ni_counter_read_forms" },
        { "case": "console/task/ni_analog_read_forms" }
      ]
    },
    {
      "sequence_name": "other",
      "sequence_order": "asynchronous",
      "pool_size": 3,
      "tests": [
        { "case": "console/layout/mosaic_operations" },
        { "case": "console/layout/keyboard_shortcuts" },
        { "case": "console/documentation" }
      ]
    }
  ]
}
